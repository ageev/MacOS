{
    "description": "Ctrl+Shift (any order) => Ctrl+Option+Space (Change keyboard layout)",
    "manipulators": [
        {
            "from": {
                "modifiers": { "optional": ["any"] },
                "simultaneous": [
                    { "key_code": "left_control" },
                    { "key_code": "left_shift" }
                ]
            },
            "parameters": { "basic.simultaneous_threshold_milliseconds": 300 },
            "to": [
                {
                    "key_code": "spacebar",
                    "modifiers": ["left_control", "left_option"]
                }
            ],
            "type": "basic"
        }
    ]
}
++++++++++++++++++++
{
    "description": "Win Key to Spotlight",
    "manipulators": [
        {
            "from": {
                "key_code": "left_option",
                "modifiers": { "optional": ["any"] }
            },
            "to": [
                {
                    "key_code": "left_option",
                    "lazy": true
                }
            ],
            "to_if_alone": [{ "apple_vendor_keyboard_key_code": "spotlight" }],
            "type": "basic"
        }
    ]
}
